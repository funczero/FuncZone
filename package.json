{
  "name": "funczone",
  "version": "1.0.0",
  "description": "Bot Discord modular com suporte a comandos, eventos e setup automatizado",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "ts-node src/scripts/preinstall.ts",
    "build": "tsc",
    "start": "npm run preinstall && npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts"
  },
  "keywords": ["discord", "bot", "typescript", "modular", "automation"],
  "author": "Edimar",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "moment-timezone": "^0.5.40",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
