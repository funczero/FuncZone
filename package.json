{
  "name": "funczone",
  "version": "1.0.0",
  "description": "Boas-vindas autom√°ticas e alertas de Boost para meu servidor no Discord.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm install tsconfig-paths",
    "postinstall": "ts-node ./src/scripts/preinstall.ts",
    "build": "./node_modules/.bin/tsc --project tsconfig.build.json",
    "build:clean": "rm -rf dist && npm run build",
    "start": "npm run prepare && npm run build && node -r tsconfig-paths/register ./dist/index.js",
    "dev": "npm run prepare && ts-node --require tsconfig-paths/register ./src/index.ts",
    "lint": "eslint . --ext .ts"
  },
  "keywords": ["discord", "bot", "typescript", "modular", "automation"],
  "author": "funczero",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "moment-timezone": "^0.5.40",
    "typescript": "^5.4.5",
    "winston": "^3.11.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2"
  }
}
