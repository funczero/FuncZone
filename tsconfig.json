{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"]
      "@utils/*": ["utils/*"]
      "@client/*": ["client/*"]
      "@config/*": ["config/*"]
      "@commands/*": ["commands/*"]
    }
  },
  "include": ["src", "assets", "utils",
    "client", "config", "commands"]
}
